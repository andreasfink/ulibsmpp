#ulibsmpp pkg-config source file

prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: ulibsmpp
Cflags: -I${includedir}
Libs: -L${libdir} -l:libulibsmpp.@SHARED_LIBRARY_EXTENSION@.@MAJORVER@.@MINORVER@

Description: universal SMPP library
Version: @VERSION@
Requires: ulib


